	"tokenColors": [
		{
			"scope": [
				"variable",
				"string constant.other.placeholder",
				"invalid.deprecated",
				"keyword",
				"storage.type",
				"storage.modifier"
			],
			"settings": {
				"foreground": "#00F7FF"
			}
		},
		{
			"scope": [
				"variable",
